<div class="{{styles.block}}">
    {{#each buttons}}
        <div class="{{../styles.buttonWrapper}}">
            <button class="{{../buttonStyles.paginator}} {{this.name}}" page="{{this.value}}">
                <span class="{{../textStyles.text}}">{{this.value}}</span>
            </button>
        </div>
    {{/each}}
</div>

