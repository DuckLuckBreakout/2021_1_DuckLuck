{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "google"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "complexity": ["error", { "max": 4 }],
        "no-var": 1,
        "no-eval": "error",
        "indent": ["error", 2],
        "quotes": ["error", "single"],
        "space-before-function-paren": ["error", "never"],
        "padded-blocks": ["error", "always"],
        "prefer-arrow-callback": [0, { "allowNamedFunctions": true }],
        "func-names": ["error", "never"]
    }
}
