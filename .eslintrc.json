{
    "parser": "babel-eslint",
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "google"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "semi": "error",
        "camelcase": "error",
        "no-console": ["error", { "allow": ["warn", "error"] }],
        "no-invalid-this": "off",
        "complexity": ["error", { "max": 4 }],
        "no-var": 1,
        "no-eval": "error",
        "indent": ["error", 4],
        "quotes": ["error", "single"],
        "space-before-function-paren": ["error", "never"],
        "prefer-arrow-callback": [0, { "allowNamedFunctions": true }],
        "func-names": ["error", "never"],
        "max-len": ["error", { "code": 120 }]
    }
}
